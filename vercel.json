{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/favicon/(.*)",
            "dest": "/public/favicon/$1"
        },
        {
            "src": "/audio/(.*)",
            "dest": "/public/audio/$1"
        },
        {
            "src": "/documents/(.*)",
            "dest": "/public/documents/$1"
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(css|js)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Personal Portfolio",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "${VERCEL_URL}",
        "ASSET_URL": "${VERCEL_URL}",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "DB_CONNECTION": "sqlite",
        "DB_DATABASE": "/tmp/database.sqlite"
    },
    "buildCommand": "bash ./build-vercel.sh",
    "outputDirectory": "public",
    "crons": [
        {
            "path": "/api/schedule.php",
            "schedule": "0 * * * *"
        }
    ]
}

